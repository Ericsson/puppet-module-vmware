{
  "name": "gillarkod-vmware",
  "version": "1.6.1",
  "author": "gillarkod",
  "summary": "Manage VMwaretools",
  "description": "Install and manage vmwaretools",
  "license": "Apache-2.0",
  "source": "https://github.com/gillarkod/puppet-module-vmware.git",
  "project_page": "https://github.com/gillarkod/puppet-module-vmware",
  "issues_url": "https://github.com/gillarkod/puppet-module-vmware/issues",
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "SLES",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "SLED",
      "operatingsystemrelease": [
        "11",
        "12"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "dependencies": [
    { "name": "puppetlabs/apt", "version_requirement": ">= 1.4.2 < 2.0.0" },
    { "name": "puppetlabs/inifile", "version_requirement": ">= 1.4.2 < 2.0.0" },
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.10.0 < 6.0.0" },
    { "name": "voxpupuli/zypprepo", "version_requirement": ">= 1.0.2 < 2.0.0" }
  ]
}
